{
    "version": 3,
    "public": true,
    "name": "wafrn",
    "images": {
      "domains": ["media.wafrn.net"],
      "sizes": [640, 750, 828, 1080, 1200],
      "minimumCacheTTL": 60,
      "formats": ["image/webp", "image/avif"]
    },
    "rewrites": [
        { "source": "/(.*)", "destination": "/api" }
      ],
      "functions": {
        "api/index.js": {
          "includeFiles": "dist/wafrn/browser/**"
        }
      }
  }
  